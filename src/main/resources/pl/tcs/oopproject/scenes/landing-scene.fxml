<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="480.0" prefWidth="800.0" stylesheets="@../styles/landing-scene.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.tcs.oopproject.view.sceneControllers.LandingSceneController">
   <children>
      <VBox id="button-vbox" alignment="TOP_RIGHT" prefHeight="400.0" prefWidth="190.0" spacing="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <Button fx:id="SignUpButton" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#SignUpClicked" prefHeight="24.0" prefWidth="131.0" text="Zarejestruj się" />
            <Button fx:id="LogInButton" mnemonicParsing="false" onMouseClicked="#LogInClicked" text="Zaloguj się" />
         </children>
         <padding>
            <Insets bottom="20.0" left="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
